datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum MealType {
  MORNING
  AFTERNOON
  EVENING
  FULL
}

enum Gender {
  MALE
  FEMALE
}

enum UserRole {
  ADMIN
  RESIDENT
  MESS
}

model User {
  id          String   @id() @default(cuid())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt()
  name        String
  phoneNumber String   @unique()
  password    String
  gender      Gender
  mealType    MealType
  role        UserRole
}
