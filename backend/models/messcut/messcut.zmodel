import '../base/base.zmodel'

model Messcut extends Base {
   date DateTime
   cutType CutType
   user User @relation(fields: [userId], references: [id])
   userId Int 

  @@allow('create', true)
  @@allow('read', auth() == this || auth().role == "ADMIN")
  @@allow('update,delete', auth() == this)
}

enum CutType {  
  MORNING
  AFTERNOON
  EVENING
  FULL
}
