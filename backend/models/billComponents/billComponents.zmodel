import '../base/base'
import "../userBills/userBills"
import "../enums/billtype"

model BillComponents extends Base {
    userBill   UserBill @relation(fields: [userBillid], references: [id], onDelete: Cascade)
    userBillid String

    totalDays  Int
    amount     Float
    type       BillType

    @@allow('all', true)
}
